{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "settings": {
    "react": {
      "pragma": "h"
    }
  },
  "rules": {
    "react/prefer-stateless-function": [0, { "ignorePureComponents": true }],
    "react/prop-types": 0,
    "max-len": 0,
    "prefer-template": 0,
    "arrow-body-style": 0,
    "arrow-parens": [0],
    "prefer-const": [1]
  }
}
